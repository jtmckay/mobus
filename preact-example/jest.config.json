{
    "collectCoverage": true,
    "collectCoverageFrom": [],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "api/generated-clients",
      "dist",
      "src/mobx-rxjs",
      "src/api/soloNodeApi",
      "src/test"
    ],
    "coverageReporters": ["lcov", "cobertura", "html", "text-summary", "text"],
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "testTimeout": 1000,
    "testPathIgnorePatterns": ["/generator"],
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  }
  